{
  "name": "reciperef",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clear-build-cache": "rm -rf .cache/ public/recipes/dist/",
    "dev": "env NODE_ENV=dev parcel -p 8090 src/index.html",
    "prod-build": "rimraf public/recipes/dist && rimraf .cache && npm run build-client && npm run start",
    "build-client": "parcel build src/index.html -d public/recipes/dist --public-url /recipes/dist",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "tslint --project .",
    "test": "jest --env=jsdom",
    "test-check": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc --noEmit",
    "start": "pm2 start server/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.1",
    "@types/reach__router": "^1.2.6",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "cross-env": "^6.0.3",
    "jest": "^24.9.0",
    "node-sass": "^4.13.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "sass": "^1.24.4",
    "serve-static": "^1.14.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "axios": "^0.19.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "pm2": "^5.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  }
}
